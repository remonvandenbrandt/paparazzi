<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="1" ground_alt="0" lat0="51.990634" lon0="4.376789" max_dist_from_home="15" name="Waypoint move" security_height="1">
  <header>

 #include "autopilot.h"
 #include "generated/modules.h"
#include "modules/object_detection_opencv/object_detection.h"
#include "firmwares/rotorcraft/stabilization/stabilization_attitude_rc_setpoint.h"
//Enable advanced electrical power functions
    #include "subsystems/electrical.h"
// Incluse airframe.h To be able to use specific variables
    #include "generated/airframe.h"
</header>
  <waypoints>
    <waypoint lat="51.990676" lon="4.376778" name="0"/>
    <waypoint lat="51.990642" lon="4.376827" name="1"/>
    <waypoint lat="51.990623" lon="4.376744" name="2"/>
    <waypoint lat="51.990613" lon="4.376776" name="3"/>
    <waypoint lat="51.990633" lon="4.376735" name="4"/>
    <waypoint lat="51.990670" lon="4.376801" name="5"/>
    <waypoint lat="51.990663" lon="4.376746" name="6"/>
    <waypoint lat="51.990660" lon="4.376812" name="7"/>
    <waypoint lat="51.990616" lon="4.376751" name="8"/>
    <waypoint lat="51.990618" lon="4.376790" name="9"/>
    <waypoint lat="51.990651" lon="4.376818" name="10"/>
    <waypoint lat="51.990657" lon="4.376730" name="11"/>
    <waypoint lat="51.990642" lon="4.376724" name="12"/>
    <waypoint lat="51.990669" lon="4.376762" name="13"/>
    <waypoint lat="51.990623" lon="4.376802" name="14"/>
    <waypoint lat="51.990630" lon="4.376820" name="15"/>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint name="CLIMB" x="0.0" y="5.0"/>
    <waypoint name="STDBY" x="-1.0" y="-4.0"/>
    <waypoint name="TD" x="5.6" y="-10.9"/>
	<waypoint name="here" x="0" y="0"/>

    <waypoint lat="51.990635" lon="4.376836" name="FA1"/>
    <waypoint lat="51.990606" lon="4.376760" name="FA2"/>
    <waypoint lat="51.990652" lon="4.376712" name="FA3"/>
    <waypoint lat="51.990681" lon="4.376791" name="FA4"/>
    <waypoint lat="51.990650" lon="4.376788" name="center"/>
  </waypoints>
  <sectors>
    <sector color="red" name="Flight_Area">
      <corner name="FA1"/>
      <corner name="FA2"/>
      <corner name="FA3"/>
      <corner name="FA4"/>
    </sector>
  </sectors>
  <!--exceptions>
	<exception cond="getObstacle()->w > 250" deroute="cvtest"/>
  </exceptions-->
  <blocks>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <call fun="NavSetWaypointHere(WP_HOME)"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <call fun="NavSetWaypointHere(WP_STDBY)"/>
      <call fun="NavSetWaypointHere(WP_TD)"/>
      <call fun="NavSetAltitudeReferenceHere()"/>
    </block>
    <block name="Holding point">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Start Engine">
      <call fun="NavResurrect()"/>
    </block>
    <block name="Takeoff" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="stateGetPositionEnu_f()->z > 1.0" deroute="Standby"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay climb="0.5" vmode="climb" wp="CLIMB"/>
    </block>
    <block name="Standby" strip_button="Standby" strip_icon="home.png">
      <stay wp="STDBY"/>
    </block>
   <block name="forward">
	<exception cond="getObstacle()->w > 250" deroute="obstacle"/>
	<exception cond="! InsideFlight_Area(GetPosX(), GetPosY())" deroute="return"/>
   	<attitude pitch="-1" roll="0" alt="1"/>
    </block>
    <block name="return">
	<attitude pitch="0" roll="0" alt="1"/>
	<call fun="NavSetWaypointHere(WP_here)"/>
	<call fun="nav_set_heading_rad((stateGetNedToBodyEulers_i()->psi)+1.4)"/>
        <!--stay wp="here"/-->
	<exception cond="NavBlockTime() > 2" deroute="forward"/>
    </block>
   <block name="obstacle">
	<attitude pitch="0" roll="0" alt="1"/>
	<call fun="NavSetWaypointHere(WP_here)"/>
	<call fun="nav_set_heading_rad((stateGetNedToBodyEulers_i()->psi)+0.6)"/>
	<!--stay wp="here"/-->
	<exception cond="NavBlockTime() > 2" deroute="forward"/>
   </block>

    <block name="gotoCENTER">
      <call fun="nav_set_heading_towards_waypoint(WP_0)"/>
      <go wp="0"/>
    </block>

    <block name="stayStandby">
      <stay wp="0"/>
    </block>
    <block name="land here" strip_button="Land Here" strip_icon="land-right.png">
      <call fun="NavSetWaypointHere(WP_TD)"/>
    </block>
    <block name="land">
      <go wp="TD"/>
    </block>
    <block name="flare">
      <exception cond="NavDetectGround()" deroute="Standby"/>
      <exception cond="!nav_is_in_flight()" deroute="landed"/>
      <call fun="NavStartDetectGround()"/>
      <stay climb="-0.8" vmode="climb" wp="TD"/>
    </block>
    <block name="landed">
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
